#include <iostream>

using namespace std;

int main()
{
    int a, b, c;

    cout << "Primeiro valor: ";
    cin >> a;
    cout << "Segundo valor: ";
    cin >> b;
    cout << "Terceiro valor: ";
    cin >> c;

    if(a <= b && a <= c)
    {
        cout << a << endl;
    } else if (b <= a && b <= c)
    {
        cout << b <<endl;
    } else if (c <= b && c <= b)
    {
        cout << c <<endl;
    }

    return 0;
}
